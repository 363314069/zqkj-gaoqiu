<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zqkj.dao.mapper.ModuleDao">
	<sql id="insert_field">
		`ParentId`,`GUID`,`OrganizationGUID`,`IndexId`,`Name`,`Link`,`Param`,`Icon`,`Sort`,`Level`,`IsParent`,`Type`,`State`,`Remark`,`Creator`,`CreateTime`,`CheckUser`,`CheckTime`,`ExtInt`,`ExtStr`
	</sql>
	<sql id="query_field">
		`ID`,`ParentId`,`GUID`,`OrganizationGUID`,`IndexId`,`Name`,`Link`,`Param`,`Icon`,`Sort`,`Level`,`IsParent`,`Type`,`State`,`Remark`,`Creator`,`CreateTime`,`CheckUser`,`CheckTime`,`ExtInt`,`ExtStr`
	</sql>
	<sql id="query_list_field">
		`ID`,`ParentId`,`GUID`,`OrganizationGUID`,`IndexId`,`Name`,`Link`,`Param`,`Icon`,`Sort`,`Level`,`IsParent`,`Type`,`State`,`Creator`,`CreateTime`,`CheckUser`,`CheckTime`
	</sql>
	<sql id="where">
			<if test="id != null">
				and ID = #{id}
			</if>
			<if test="parentId != null">
				and ParentId = #{parentId}
			</if>
			<if test="guid != null">
				and GUID = #{guid}
			</if>
			<if test="organizationGUID != null">
				and OrganizationGUID = #{organizationGUID}
			</if>
			<if test="indexId != null">
				and IndexId = #{indexId}
			</if>
			<if test="name != null">
				and Name = #{name}
			</if>
			<if test="link != null">
				and Link = #{link}
			</if>
			<if test="param != null">
				and Param = #{param}
			</if>
			<if test="icon != null">
				and Icon = #{icon}
			</if>
			<if test="sort != null">
				and Sort = #{sort}
			</if>
			<if test="level != null">
				and Level = #{level}
			</if>
			<if test="isParent != null">
				and IsParent = #{isParent}
			</if>
			<if test="type != null">
				and Type = #{type}
			</if>
			<if test="state != null">
				and State = #{state}
			</if>
			<if test="creator != null">
				and Creator = #{creator}
			</if>
			<if test="createTime != null">
				and CreateTime = #{createTime}
			</if>
			<if test="checkUser != null">
				and CheckUser = #{checkUser}
			</if>
			<if test="checkTime != null">
				and CheckTime = #{checkTime}
			</if>
	</sql>
</mapper>