@charset "utf-8";
html, body, div, applet, object, iframe, h1, h2, h3, h4, h5, h6, blockquote, pre, abbr, acronym, address, cite, code, del, dfn, img, ins, kbd, q, s, samp, strike, sub, sup, tt, var, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, p{
	 margin:0; padding:0; 
	 border:0; outline:0; 
	 font:"微软雅黑","宋体",Arial,tahoma,Srial,helvetica,sans-serif; 
	 -webkit-text-size-adjust:none; 
	 font-weight:inherit;	
	}
body,html{width: 100%; height: 100%;}
img {border:0;width:100%;display:block;}
ol,ul {list-style:none}
input {font-family:Microsoft YaHei,Arial;outline:none;}
input:focus {outline:none;color:#aaa;}
table {border-collapse:collapse;border-spacing:0;}
body{ background-color: #f5f5f5;}
a:link,a:visited,a:hover {text-decoration:none;}
.layui-elem-field legend{padding: 20px 0;}
#rrapp{max-width: 800px; margin: 0 auto;}

.hue_red,a.hue_red {color:#d12227;}
.hue_red1,a.hue_red1 {color:#f00;}
.hue_gre,a.hue_gre {color:#2eb05f;}
.hue_gray,a.hue_gray {color:#bbb;}
.alignC {text-align:center;}
.clearfix { padding: 0; margin: 0;}
.clearfix:after {content:".";display:block;height:0;clear:both;visibility:hidden;}
.clearb {clear:both;}
.floatleft{ float:left;}
.floatright{ float:right;}
.none{ display:none;}
.wrap{padding: 20px;}
.wh90{ 	
	margin: 0 auto; 	
	width: 1280px;
}