</html>
<script>
    var goodsGuid = getQueryString("guid");
    var userGuidsT = new Array();
	$(function () {
		selectUser();
	});
	function selectUser(){
		$(".list").empty();
		$.ajax({
			type : "POST",
			url : T.serverurl + "/pay/order/list",
			headers : {token:T.token},
			data :  {"orderBy" : "createTime desc","state":1,"goodsGuid":goodsGuid},
			success : function(r) {
				if(r.code == 0){
					$.each(r.data,function (i, obj) {
						userGuidsT.push(obj.userGuid);
					});
					if(userGuidsT != null && userGuidsT.length > 0){
						$.ajax({
							type : "POST",
							url : T.serverurl + "/security/user/listByGuids",
							headers : {token:T.token},
							traditional: true,
							data : {"guids":userGuidsT},
							success : function(ruser) {
								if(r.code == 0){
									$.each(ruser.data,function (iuser, objuser) {
										var li = $('<li>');
										var li_ul = $('<ul>');
										li_ul.append('<li>'+objuser.name+'</li>');
										li.append(li_ul);
										$(".list").append(li);
									});
								}
							}
						});
					}
				}
			}
		});
	}

	function downloadUser(){
		if(userGuidsT != null && userGuidsT.length > 0) {
			$.ajax({
				type: "get",
				url: T.serverurl + "/security/user/exportexcel",
				headers: {token: T.token},
				traditional: true,
				data: {"guids": userGuidsT},
				success: function (r) {

				}
			});
		}else{
			alert("没有可下载的人员");
		}
	}

</script>