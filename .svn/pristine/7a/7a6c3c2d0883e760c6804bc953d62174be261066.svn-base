/*
样式*/
ul {
    margin: 0;
    list-style: none;
    padding: 0;
    width: 100%;
}

.register {
    position: fixed;
    width:90%;
    padding: 15px;
    background-color: #FFF;
    border: 1px solid #CCC;
    border-radius: 5px;
    position:fixed;
    left:50%;
    top:50%;
    transform:translate(-50%,-50%);
    z-index:1000;
    opacity: 0.8;
}

.login-wrap li {
    display: flex;
    margin: 10px 0;
}

.login-wrap  label,.login-wrap  input {
    display: block;
    float: left;
    font-size: 14px;
    box-sizing: border-box;
    color: #333;
}

.login-wrap  label {
    width: 22%;
    margin-right: 10px;
    text-align: right;
}
.login-wrap{
    width: 90%;
    margin: 0 auto;
}
.login-wrap input {
    width: 75%;
    padding: 8px;
    line-height: 1;
    outline: none;
    position: relative;
    border-radius: 5px;
    border: 1px solid #ccc;
}

.login-wrap  input.code {
    width: 100px;
}

input.verify {
    width: 100px;
    margin-left: 10px;
}

input.disabled {
    background-color: #CCC !important;
}

.register  input[type=button] {
    border: none;
    color: #FFF;
    background-color: #E64145;
    border-radius: 4px;
    cursor: pointer;
    width: 100px;
}
.register  .avatar {
    position: fixed;
    margin-top: -20px;
    position: absolute;
    top: -60px;
    left: 50%;
    overflow: hidden;
    width: 130px;
    height: 130px;
    margin-left: -65px;
    border: 10px solid #fff;
    border-radius: 50%;
    box-shadow: 0 1px 5px #ccc;
}