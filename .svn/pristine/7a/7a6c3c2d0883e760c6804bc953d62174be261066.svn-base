#dialogBg {
    width: 100%;
    height: 100%;
    background-color: #000000;
    opacity: .6;
    filter: alpha(opacity=60);
    position: fixed;
    top: 0;
    left: 0;
    z-index: 9999;
    display: none;
}

#dialog {
    width:90%;
    height: 380px;
    margin: 0 auto;
    display: none;
    background-color: #ffffff;
    position: absolute;
    top: 0%;
    left: 50%;
